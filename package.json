{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/**/test/**/*.test.*"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [
      "@virtualpatterns/mablung-source-map-support/install"
    ],
    "verbose": true
  },
  "babel": {
    "presets": [
      [
        "@virtualpatterns/babel-preset-mablung-makefile",
        {
          "header": {
            "exclude": [
              "source/sandbox/**/*",
              "source/test/**/*"
            ]
          }
        }
      ]
    ]
  },
  "bin": {
    "check-dependency": "./release/command/check-dependency.js"
  },
  "c8": {
    "all": true,
    "exclude": [
      "release/header",
      "release/sandbox",
      "release/test/command/resource",
      "release/test/library/resource"
    ],
    "reporter": [
      "html"
    ],
    "src": "release"
  },
  "description": "TBD",
  "eslintConfig": {
    "extends": "@virtualpatterns/eslint-config-mablung-makefile"
  },
  "exports": {
    ".": "./release/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "release/command",
    "release/library",
    "release/index.js"
  ],
  "license": "GPL-3.0+",
  "name": "@virtualpatterns/mablung-check-dependency",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/mablung-check-dependency"
  },
  "type": "module",
  "version": "0.0.4-0",
  "dependencies": {
    "@kba/makefile-parser": "0.0.6",
    "@pwn/is": "^1.0.2",
    "@virtualpatterns/mablung-source-map-support": "0.0.8",
    "commander": "^8.1.0",
    "deepmerge": "^4.2.2",
    "depcheck": "^1.4.2",
    "find": "^0.3.0",
    "fs-extra": "^10.0.0",
    "jsonpath": "^1.1.1",
    "minimatch": "^3.0.4",
    "node-json-transform": "^1.1.2",
    "pug-lexer": "^5.0.1",
    "pug-load": "^3.0.0",
    "pug-parser": "^6.0.0"
  },
  "devDependencies": {
    "@virtualpatterns/babel-preset-mablung-makefile": "0.0.10",
    "@virtualpatterns/eslint-config-mablung-makefile": "0.0.10",
    "@virtualpatterns/mablung-makefile": "0.0.19",
    "@virtualpatterns/mablung-worker": "0.0.2-40",
    "ava": "^3.15.0"
  }
}
