{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/test/**/*.test.*"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [
      "@virtualpatterns/mablung-source-map-support/install"
    ],
    "verbose": true
  },
  "bin": {
    "check-dependency": "./release/command/check-dependency.js"
  },
  "c8": {
    "all": true,
    "exclude": [
      "release/sandbox",
      "release/test",
      "source/sandbox",
      "source/test"
    ],
    "reporter": [
      "html"
    ]
  },
  "description": "TBD",
  "exports": {
    ".": "./release/index.js",
    "./package.json": "./package.json"
  },
  "license": "GPL-3.0+",
  "name": "@virtualpatterns/mablung-check-dependency",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/mablung-check-dependency"
  },
  "scripts": {
    "refresh": "rm -Rf node_modules package-lock.json && npm install",
    "upgrade:10": "npm-check-updates --upgrade",
    "upgrade:20": "shx rm -f package-lock.json",
    "upgrade:30": "npm install",
    "upgrade": "run-s --silent upgrade:*",
    "clean:10": "shx rm -Rf coverage process release",
    "clean": "run-s --silent clean:*",
    "lint:10": "eslint --ignore-path .gitignore source",
    "lint": "run-s --silent lint:*",
    "build:10": "babel source --copy-files --extensions .js --ignore ./source/test/command/resource,./source/test/library/resource --keep-file-extension --out-dir release --source-maps",
    "build": "run-s --silent clean lint build:*",
    "test:10": "c8 ava",
    "test": "run-s --silent build \"test:10 {@}\" --",
    "run:10": "node --no-warnings --unhandled-rejections=strict",
    "run": "run-s --silent build \"run:10 {@}\" --",
    "pre:push:10": "git add coverage release",
    "pre:push:20": "git commit --message=\"post-test\"",
    "pre:push": "run-s --silent test pre:push:*",
    "push:10": "./release/command/check-dependency.js",
    "push:20": "npm version prerelease",
    "push:30": "git push origin master",
    "push": "run-s --silent pre:push push:*"
  },
  "type": "module",
  "version": "0.0.1-25",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@virtualpatterns/mablung-worker": "git+https://github.com/virtualpatterns/mablung-worker.git",
    "ava": "^3.15.0",
    "babel-eslint": "^10.1.0",
    "c8": "^7.4.0",
    "eslint": "^7.17.0",
    "eslint-plugin-babel": "^5.3.1",
    "npm-check-updates": "^10.2.5",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3"
  },
  "dependencies": {
    "@pwn/is": "^1.0.2",
    "@virtualpatterns/mablung-source-map-support": "git+https://github.com/virtualpatterns/mablung-source-map-support.git",
    "commander": "^6.2.1",
    "deepmerge": "^4.2.2",
    "depcheck": "^1.3.1",
    "fs-extra": "^9.0.1",
    "json5": "^2.1.3",
    "jsonpath": "^1.0.2",
    "minimatch": "^3.0.4",
    "node-json-transform": "^1.1.1",
    "pug-lexer": "^5.0.0",
    "pug-load": "^3.0.0",
    "pug-parser": "^6.0.0"
  }
}
